pipeline {
    agent {
        node {
            label "built-in"
            customWorkspace '/mnt/rupaaa'
        }
    }
    stages {
        stage ('git') {
            steps {
                sh "yum install git -y"
            }
        }
        
        stage ('config') {
            steps {
                sh "git config --global user.Rupesh"
                sh "git config --global user.abc@gmail.com"
            }
        }
    }
}
